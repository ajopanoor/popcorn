menu "Rpmsg drivers"

# RPMSG always gets selected by whoever wants it
config RPMSG
	tristate
	select VIRTIO
	select VIRTUALIZATION

# RPMSG always gets selected by whoever wants it
config RPMSG_CLIENT
	tristate "Dummy rpmsg client driver for testing rpmsg -- loadable module" 
	depends on RPMSG && m
endmenu
